#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#ifndef ERECAUDACION_H_
#define ERECAUDACION_H_

typedef struct {
	int idRecaudacion;
	int idContribuyente;
	char tipo[15];
	int mes;
	float importe;
	int isEmpty;
} eRecaudacion;


#endif /* ERECAUDACION_H_ */
